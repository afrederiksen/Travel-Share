<body>
    <main role="main">
        <center>
            <div class="starter-template">
                <div id="map"></div>
            </div>
        </center>

    </main><!-- /.container -->
    <script>
        var base = L.tileLayer('https://api.mapbox.com/styles/v1/{id}/tiles/{z}/{x}/{y}?access_token={accessToken}', {
            attribution: 'Map data &copy; <a href="https://www.openstreetmap.org/">OpenStreetMap</a> contributors, <a href="https://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>, Imagery Â© <a href="https://www.mapbox.com/">Mapbox</a> Adam Frederiksen 2020',
            maxZoom: 19,
            id: 'mapbox/dark-v10',
            tileSize: 512,
            zoomOffset: -1,
            accessToken: ''
        });
        var map = L.map('map', {
            layers: [base],
            center: new L.LatLng(48.5, -4.5),
            zoom: 5
        });
        //Mutliyear
        console.log({{trip.coordinates.long}});
       L.marker([{{trip.coordinates.long}}, {{trip.coordinates.lat}}]).addTo(map).bindPopup("<b>{{trip.title}}</b><br>{{trip.yearsTraveled}}");
        // create fullscreen control
        var fsControl = L.control.fullscreen();
        // add fullscreen control to the map
        map.addControl(fsControl);
        // detect fullscreen toggling
        map.on('enterFullscreen', function() {
            if (window.console) window.console.log('enterFullscreen');
        });
        map.on('exitFullscreen', function() {
            if (window.console) window.console.log('exitFullscreen');
        });
    </script>
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script>
        window.jQuery || document.write('<script src="jquery.slim.min.js"><\/script>')
        $(window).on("resize", function() {
            $("#map").height($(window).height());
            map.invalidateSize();
        }).trigger("resize");
    </script>
